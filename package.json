{
  "name": "fly-action",
  "version": "1.0.0",
  "description": "GitHub Action to setup Fly CLI",
  "main": "lib/index.js",
  "post": "lib/post.js",
  "scripts": {
    "build": "npm run format && tsc && ncc build dist/index.js -o lib && mkdir -p lib/post_temp && ncc build dist/post.js -o lib/post_temp && mv lib/post_temp/index.js lib/post.js && rm -rf lib/post_temp",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "postinstall": "npm run format"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.1",
    "@actions/http-client": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^9.35.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.2",
    "typescript": "^5.2.2",
    "husky": "^8.0.0"
  },
  "engines": {
    "node": ">=16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfrog/fly-action.git"
  },
  "keywords": [
    "github",
    "action",
    "fly"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfrog/fly-action/issues"
  },
  "homepage": "https://github.com/jfrog/fly-action#readme"
}