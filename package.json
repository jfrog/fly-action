    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.x"
