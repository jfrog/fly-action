{
  "name": "flyfrog-action",
  "version": "1.0.0",
  "description": "GitHub Action to setup FlyFrog CLI",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
